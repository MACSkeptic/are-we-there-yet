{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "are we there yet?!",
  "web_accessible_resources": [
    "blue-grumpy.png",
    "artsy-grumpy.png",
    "yellow-grumpy.png",
    "green-grumpy.png",
    "happy-grumpy.png"
  ],
  "page_action": {
    "default_icon": "tiny-grumpy.png",
    "default_title": "don't be so grumpy"
  },
  "permissions": [
    "storage",
    "notifications",
    "https://github.com/"
  ],
  "options_page": "options.html",
  "content_scripts": [{
    "matches": ["https://github.com/*"],
    "run_at": "document_end",
    "js": ["inject.js"]
  }]
}
